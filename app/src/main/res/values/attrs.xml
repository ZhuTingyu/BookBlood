<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FamilyTreeView">
        <attr name="TreeView_LineWidth" format="dimension"/>
        <attr name="TreeVie_LineColor" format="color"/>
        <attr name="TreeView_ShowGeneration" format="integer"/>
        <attr name="TreeView_MaxGeneration" format="integer"/>
        <attr name="TreeView_isHaveCurrent" format="boolean"/>
        <attr name="TreeView_isHorizontal" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="SimpleTitleView">
        <attr name="simpleTitleView_DefaultTextColor" format="color"/>
        <attr name="simpleTitleView_PressTextColor" format="color"/>
        <attr name="simpleTitleView_image" format="reference|color"/>
    </declare-styleable>
    
    <declare-styleable name="LineInputView">
        <attr name="lineInputView_Weight" format="integer"/>
        <attr name="lineInputView_LeftTextColor" format="color"/>
        <attr name="lineInputView_RightTextColor" format="color"/>
        <attr name="lineInputView_LeftTextSize" format="dimension"/>
        <attr name="lineInputView_RightTextSize" format="dimension"/>
        <attr name="lineInputView_LeftString" format="string"/>
        <attr name="lineInputView_RightString" format="string"/>
        <attr name="lineInputView_IsLookState" format="boolean"/>
        <attr name="lineInputView_DrawableRight" format="reference"/>
        <attr name="lineInputView_IsNotNull" format="boolean"/>
        <attr name="lineInputView_IsCanEdit" format="boolean"/>
        <attr name="lineInputView_Hint" format="string"/>
    </declare-styleable>

    <declare-styleable name="SearchTextView">
        <attr name="searchTextView_is_in_top" format="boolean"/>
    </declare-styleable>
</resources>
